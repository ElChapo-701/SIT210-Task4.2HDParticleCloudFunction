int led1 = A1;
int led2 = A2;
int led3 = A3;

void setup() {

pinMode(led1, OUTPUT);
pinMode(led2, OUTPUT);
pinMode(led3, OUTPUT);
digitalWrite(led1, LOW);
digitalWrite(led2, LOW);
digitalWrite(led3, LOW);

}

void loop() {
    Particle.function("red", led1);
    Particle.function("green", led2);
    Particle.function("blue", led3);

}

int ledToggle(String command){
    
    
    if(command == "red"){
       digitalWrite(led1, HIGH);
       digitalWrite(led2, LOW);
       digitalWrite(led3, LOW);
        return 1;
    } else if(command == "green"){
       digitalWrite(led1, LOW);
       digitalWrite(led2, HIGH);
       digitalWrite(led3, LOW);
        return 0;
    } else if(command == "blue"){
       digitalWrite(led1, LOW);
       digitalWrite(led2, LOW);
       digitalWrite(led3, HIGH);
        return 0;
    } else {
        return -1;
    }
}
